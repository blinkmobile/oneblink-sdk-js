      "dist",
      "examples",
      "test"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneblink/sdk-js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "jest": "jest",
    "pretest": "npm run eslint && npm run flow_check && npm run fixpack",
    "test": "npm run jest"
  }
}
