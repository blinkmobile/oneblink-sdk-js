{
  "name": "@oneblink/sdk",
  "description": "OneBlink SDK to serve as an entry point for all OneBlink Services in NodeJS",
  "version": "0.7.0",
  "bugs": {
    "url": "https://github.com/oneblink/sdk-js/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.692.0",
    "axios": "^0.19.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fixpack": "^3.0.6",
    "flow-bin": "^0.126.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
